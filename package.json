    "@storybook/svelte": "7.6.4",
    "@storybook/sveltekit": "7.6.4",
    "@storybook/test": "7.6.4",
    "@storybook/theming": "^7.6.9",
    "@sveltejs/adapter-auto": "^1.0.3",
    "@sveltejs/kit": "^1.29.1",
